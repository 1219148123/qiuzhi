page {
  background-color: #f8f8f8;
  font-size: 30rpx;
  font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
  min-height: 100vh;
  padding-bottom: 40rpx;
}
.cell-item {
  padding: 0 40rpx;
}
.p-style {
  line-height: 1.4em;
}
.img-style {
  height: 400rpx;
  width: 100%;
  margin: 0 auto;
}
.footer {
  margin-top: 30rpx;
  height: 80rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  color: #b5b4b9;
}
.header{
  display: flex;
  justify-content: center;
  background-color: #fff;
  margin-bottom: 20rpx;
}
.title{
  font-size: 40rpx;
  margin: 20rpx 0;
}